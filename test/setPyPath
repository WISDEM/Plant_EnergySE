# setPyPath
# USAGE: source setPyPath
# - sets Python module search path to include user modules needed for OpenMDAO
# - appends to existing $PYTHONPATH
# - NOW STARTS WITH EMPTY $PYTHONPATH

# 2014 03 27

unset PYTHONPATH
export PYTHONPATH=/c/SystemsEngr/FUSED-Wind-develop/fusedwind/src
export PYTHONPATH=$PYTHONPATH:/c/SystemsEngr/FUSED-Wind-develop/fusedwind/src/fusedwind/plant_flow
export PYTHONPATH=$PYTHONPATH:/c/SystemsEngr/FUSED-Wind-develop/fusedwind/src/fusedwind/plant_flow/py4we
export PYTHONPATH=$PYTHONPATH:/c/SystemsEngr/Plant_AEPSE_GNS/src/Plant_AEPSE/Openwind/Enterprise
export PYTHONPATH=$PYTHONPATH:/c/SystemsEngr/Plant_AEPSE_GNS/src/Plant_AEPSE/Openwind/Academic
